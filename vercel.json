{
    "builds": [
      {
        "src": "client/src/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/server/index.ts" },
      { "src": "/(.*)", "dest": "/client/index.html" }
    ]
  }
  